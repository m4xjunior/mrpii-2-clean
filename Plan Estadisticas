Query 1: Dados de Produção por Turno (com datas)

-- Buscar dados agregados por turno para uma máquina em um período
SELECT TOP 5
    cm.Cod_maquina,
    cm.Desc_maquina,
    CONVERT(VARCHAR(10), hp.Dia_productivo, 111) AS fecha,
    hp.Id_turno,
    ct.Cod_turno,
    ct.Desc_turno AS turno,
    co.Cod_operario,
    co.Desc_operario AS operario,
    
    -- Métricas OEE do turno
    fhc.OEE_C AS oee,
    fhc.Disp_C AS disponibilidad,
    fhc.Rend_C AS rendimiento,
    fhc.Cal_C AS calidad,
    
    -- Contadores de peças do turno
    SUM(hp.Unidades_ok) AS piezas_ok,
    SUM(hp.Unidades_nok) AS piezas_nok,
    SUM(hp.Unidades_repro) AS piezas_rw,
    
    -- Tempos em horas
    SUM(DATEDIFF(MINUTE, hp.Fecha_ini, hp.Fecha_fin)) / 60.0 AS total_horas,
    SUM(ISNULL(hp.PNP, 0)) / 60.0 AS horas_paros,
    
    -- Atividades (preparação vs produção)
    SUM(CASE WHEN ca.Cod_actividad = 'PREP' THEN DATEDIFF(MINUTE, hp.Fecha_ini, hp.Fecha_fin) ELSE 0 END) / 60.0 AS horas_preparacion,
    SUM(CASE WHEN ca.Cod_actividad = 'PROD' THEN DATEDIFF(MINUTE, hp.Fecha_ini, hp.Fecha_fin) ELSE 0 END) / 60.0 AS horas_produccion

FROM his_prod hp WITH (NOLOCK)
INNER JOIN cfg_maquina cm WITH (NOLOCK) ON cm.Id_maquina = hp.Id_maquina
INNER JOIN cfg_turno ct WITH (NOLOCK) ON ct.Id_turno = hp.Id_turno
LEFT JOIN cfg_operario co WITH (NOLOCK) ON co.Id_operario = hp.Id_operario
LEFT JOIN cfg_actividad ca WITH (NOLOCK) ON ca.Id_actividad = hp.Id_actividad

-- JOIN com F_his_ct para pegar métricas oficiais MAPEX
CROSS APPLY F_his_ct(
    'WORKCENTER',
    'DAY',
    'TURNO',
    DATEADD(DAY, -7, GETDATE()),
    DATEADD(DAY, 1, GETDATE()),
    0
) fhc

WHERE cm.Cod_maquina = 'DOBL5'  -- Exemplo
  AND hp.Dia_productivo >= DATEADD(DAY, -7, GETDATE())
  AND hp.Dia_productivo <= GETDATE()
  AND hp.Activo = 1
  AND fhc.WorkGroup = cm.Cod_maquina
  AND fhc.timeperiod = CONVERT(VARCHAR(10), hp.Dia_productivo, 111)
  AND fhc.desc_turno = ct.Desc_turno

GROUP BY
    cm.Cod_maquina,
    cm.Desc_maquina,
    hp.Dia_productivo,
    hp.Id_turno,
    ct.Cod_turno,
    ct.Desc_turno,
    co.Cod_operario,
    co.Desc_operario,
    fhc.OEE_C,
    fhc.Disp_C,
    fhc.Rend_C,
    fhc.Cal_C

ORDER BY hp.Dia_productivo DESC, hp.Id_turno;

