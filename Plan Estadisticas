Query 2: Listar OFs Disponíveis de uma Máquina


-- Listar todas as OFs de uma máquina (com ou sem filtro de datas)
SELECT DISTINCT
    ho.Cod_of AS codigo_of,
    ho.Desc_of AS descricao_of,
    ho.Fecha_ini AS fecha_inicio,
    ho.Fecha_fin AS fecha_fin,
    MIN(hp.Fecha_ini) AS fecha_inicio_real,
    MAX(hp.Fecha_fin) AS fecha_fin_real,
    SUM(hp.Unidades_ok + hp.Unidades_nok + hp.Unidades_repro) AS total_producido,
    ho.Unidades_planning AS total_planificado

FROM his_of ho WITH (NOLOCK)
INNER JOIN his_fase hf WITH (NOLOCK) ON ho.Id_his_of = hf.Id_his_of
INNER JOIN his_prod hp WITH (NOLOCK) ON hf.Id_his_fase = hp.Id_his_fase
INNER JOIN cfg_maquina cm WITH (NOLOCK) ON hp.Id_maquina = cm.Id_maquina

WHERE cm.Cod_maquina = 'DOBL5'  -- Exemplo
  AND ho.Activo = 1
  AND YEAR(ho.Fecha_ini) >= YEAR(GETDATE())
  -- AND hp.Fecha_ini >= @startDate  -- Opcional
  -- AND hp.Fecha_fin <= @endDate    -- Opcional

GROUP BY
    ho.Cod_of,
    ho.Desc_of,
    ho.Fecha_ini,
    ho.Fecha_fin,
    ho.Unidades_planning

ORDER BY MIN(hp.Fecha_ini) DESC;

